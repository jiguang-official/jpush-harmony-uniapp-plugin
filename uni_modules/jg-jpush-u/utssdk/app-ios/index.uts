/**
 * 设置角标数量
 */
export function setBadgeNumber(curNum : Int) : void {
	log("setBadgeNumber", curNum)
	// 调用极光推送接口设置角标
	JPUSHService.setBadge(curNum)
	// 同时调用系统接口设置角标
	if (UTSiOS.available("iOS 16.0, *")) {
		// iOS 16 及以上使用 UNUserNotificationCenter
		UNUserNotificationCenter.current().setBadgeCount(Int(curNum), withCompletionHandler = (error : NSError | null) => {
			if (error != null) {
				log("setBadgeCount error:", error)
			}
		})
	} else {
		// iOS 16 以下使用 UIApplication
		UIApplication.shared.applicationIconBadgeNumber = Int(curNum)
	}
}

/**
 * 重置角标
 */
export function resetBadge() : void {
	log("resetBadge")
	JPUSHService.resetBadge()
	// 同时调用系统接口重置角标
	if (UTSiOS.available("iOS 16.0, *")) {
		// iOS 16 及以上使用 UNUserNotificationCenter
		UNUserNotificationCenter.current().setBadgeCount(0, withCompletionHandler = (error : NSError | null) => {
			if (error != null) {
				log("resetBadge setBadgeCount error:", error)
			}
		})
	} else {
		// iOS 16 以下使用 UIApplication
		UIApplication.shared.applicationIconBadgeNumber = 0
	}
}

