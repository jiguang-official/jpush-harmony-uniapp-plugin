/**
 * 筛选有效标签。用于在 setTags/addTags 前过滤无效 tag，避免因单个无效 tag 导致整次设置失败。
 * 调用原生 JPUSHService.filterValidTags，若超出数量/总长限制则返回靠前的有效 tag。
 * @param tags 待校验的标签数组
 * @returns 有效标签数组
 */
export function filterValidTags(tags : string[]) : string[] {
	if (tags == null || tags.length == 0) return []
	const inputSet : Swift.Set<String> = new Swift.Set<String>(tags)
	const validSet = JPUSHService.filterValidTags(inputSet)
	if (validSet == null) return []
	const arr = validSet.allObjects()
	if (arr == null) return []
	const result : string[] = []
	for (let i = 0; i < arr.length; i++) {
		const obj = arr[i]
		if (obj != null && typeof obj === 'string') {
			result.push(obj as string)
		}
	}
	return result
}

